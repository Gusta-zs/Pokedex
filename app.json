{
  "expo": {
    "name": "Pokedex",
    "slug": "Pokedex",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/imagens/pokeicon.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/imagens/pokeicon.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundImage": "./src/assets/imagens/pokeicon.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.Pokedex"
    },
    "web": {
      "favicon": "./src/assets/imagens/pokeicon.jpg"
    }
  }
}
